{
    "name": "ams extension",
    "version": "0.0.1",
    "description": "AMS Extension",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "./Popup/popup.html"
    },
    "permissions": [
        "tabs",
        "storage",
        "activeTab",
        "declarativeContent"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://meet.google.com/*"
            ],
            "js": [
                "content.bundle.js",
                "jquery-3.6.0.min.js"
            ],
            "run_at": "document_idle",
            "all_frames": false
        }
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "externally_connectable": {
        "matches": [
            "*://meet.google.com/*"
        ]
    },
    "author": "Mohan Vaddi"
}